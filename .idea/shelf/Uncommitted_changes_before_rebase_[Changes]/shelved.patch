+package com.tranxortrider.deliveryrider.activities;
+
+import android.content.Intent;
+import android.os.Build;
+import android.os.Bundle;
+import android.view.View;
+import android.view.WindowManager;
+
+import androidx.appcompat.app.AppCompatActivity;
+import androidx.core.view.ViewCompat;
+import androidx.core.view.WindowCompat;
+import androidx.core.view.WindowInsetsCompat;
+
+import com.google.android.material.bottomnavigation.BottomNavigationView;
+import com.tranxortrider.deliveryrider.R;
+import com.tranxortrider.deliveryrider.batch;
+import com.tranxortrider.deliveryrider.scanner;
+import com.tranxortrider.deliveryrider.ProfileActivity;
+import com.tranxortrider.deliveryrider.EarningsActivity;
+
+/**
+ * Home screen activity with proper system insets handling
+ */
+public class HomeScreenActivity extends AppCompatActivity {
+
+    private BottomNavigationView bottomNavigationView;
+    private boolean isNavigatingAway = false;
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_home_screen);
+        
+        // Setup window insets
+        setupWindowInsets();
+        
+        // Initialize UI components
+        initializeUI();
+        
+        // Setup bottom navigation
+        setupBottomNavigation();
+    }
+    
+    /**
+     * Configure window to handle system bars and insets properly
+     */
+    private void setupWindowInsets() {
+        // For Android 10+ (API 29+), enable proper handling of gesture navigation
+        if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.Q) {
+            WindowCompat.setDecorFitsSystemWindows(getWindow(), false);
+            
+            // Prevent keyboard from pushing content up
+            getWindow().setSoftInputMode(WindowManager.LayoutParams.SOFT_INPUT_ADJUST_RESIZE);
+        }
+    }
+    
+    private void initializeUI() {
+        // Find the bottom navigation view
+        View bottomNavInclude = findViewById(R.id.bottomNavigation);
+        if (bottomNavInclude != null) {
+            bottomNavigationView = (BottomNavigationView) bottomNavInclude;
+        }
+    }
+    
+    private void setupBottomNavigation() {
+        if (bottomNavigationView != null) {
+            // Set the home tab as selected
+            bottomNavigationView.setSelectedItemId(R.id.nav_home);
+            
+            // Handle navigation item clicks
+            bottomNavigationView.setOnItemSelectedListener(item -> {
+                if (isNavigatingAway) {
+                    return false;
+                }
+                
+                int itemId = item.getItemId();
+                
+                // Handle navigation based on the selected item
+                if (itemId == R.id.nav_home) {
+                    // Already on home screen
+                    return true;
+                } else if (itemId == R.id.nav_orders) {
+                    // Navigate to batch activity instead of pending orders
+                    navigateToActivity(batch.class);
+                    return true;
+                } else if (itemId == R.id.nav_scanner) {
+                    // Navigate to scanner screen
+                    navigateToActivity(scanner.class);
+                    return true;
+                } else if (itemId == R.id.nav_earnings) {
+                    // Navigate to earnings screen
+                    navigateToActivity(EarningsActivity.class);
+                    return true;
+                } else if (itemId == R.id.nav_profile) {
+                    // Navigate to profile screen
+                    navigateToActivity(ProfileActivity.class);
+                    return true;
+                }
+                
+                return false;
+            });
+        }
+    }
+    
+    /**
+     * Navigate to an activity with proper transition
+     */
+    private void navigateToActivity(Class<?> targetActivity) {
+        if (isNavigatingAway) {
+            return;
+        }
+        
+        isNavigatingAway = true;
+        Intent intent = new Intent(this, targetActivity);
+        intent.setFlags(Intent.FLAG_ACTIVITY_NO_ANIMATION);
+        startActivity(intent);
+        
+        // Apply custom transition to avoid flashing
+        overridePendingTransition(R.anim.fade_in, R.anim.fade_out);
+    }
+    
+    /**
+     * Apply insets to the bottom navigation bar
+     */
+    private void configureBottomNavigation() {
+        if (bottomNavigationView != null) {
+            // Apply insets
+            ViewCompat.setOnApplyWindowInsetsListener(bottomNavigationView, (v, insets) -> {
+                int navBarInset = insets.getInsets(WindowInsetsCompat.Type.navigationBars()).bottom;
+                
+                // If we have system navigation (either buttons or gesture), add padding
+                if (navBarInset > 0) {
+                    bottomNavigationView.setPadding(
+                        bottomNavigationView.getPaddingLeft(),
+                        bottomNavigationView.getPaddingTop(),
+                        bottomNavigationView.getPaddingRight(),
+                        Math.max(bottomNavigationView.getPaddingBottom(), navBarInset)
+                    );
+                }
+                return insets;
+            });
+        }
+    }
+    
+    @Override
+    protected void onResume() {
+        super.onResume();
+        
+        // Reset navigation flag when returning to this activity
+        isNavigatingAway = false;
+        
+        // Configure bottom navigation with appropriate insets
+        configureBottomNavigation();
+        
+        // Update the selected tab when returning to this activity
+        if (bottomNavigationView != null) {
+            bottomNavigationView.setSelectedItemId(R.id.nav_home);
+        }
+    }
+    
+    @Override
+    public void finish() {
+        super.finish();
+        overridePendingTransition(R.anim.fade_in, R.anim.fade_out);
+    }
+} 
